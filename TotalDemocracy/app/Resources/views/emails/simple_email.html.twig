{% extends "emails/base_email.html.twig" %}

{% block body %}
    {% if header is defined %}
        <h2>{{ header }}</h2>
    {% endif %}
    {% if paragraphs is defined %}
        {% for paragraph in paragraphs %}
            <p>{{ paragraph | raw }}</p>
        {% endfor %}
    {% endif %}
    {% if html is defined %}
        {{ html | raw }}
    {% endif %}

{% endblock %}